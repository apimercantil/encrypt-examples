import com.mercantilbanco.epgw.commons.Util;

public class cryptoaes {
	public static void main(String[] args) {
		Util util = new Util();
		String encrypdata = util.encrypt("499","A9279120481620090622AA30");
		System.out.println("encrypdata = " + encrypdata );
		
		String encrypdata1 = util.decrypt("A9279120481620090622AA30", "48puf7tJdJv8grWMpNIhUw==");
		System.out.println("encrypdata1 = " + encrypdata1 );		
		
		
	}
}
